{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "strict": "off",
      "global-require": "off",
      "no-undef": "off",
      "class-methods-use-this": "off",
      "no-param-reassign": "off"
    }
}
